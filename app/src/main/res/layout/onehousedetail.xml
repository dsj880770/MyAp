<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/title_Bar_detail"
        layout="@layout/title_house_detail"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/title_Bar_detail">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <fragment
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:name="com.bwf.p1_landz.ui.onevilla.fragment.Fragment_detail_fragment01"
                android:id="@+id/detail_frament1"
                />
            <fragment
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:name="com.bwf.p1_landz.ui.onevilla.fragment.Fragment_detail_fragment02"
                android:id="@+id/detail_frament2"/>
            <fragment
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:name="com.bwf.p1_landz.ui.onevilla.fragment.Fragment_detail_fragment03"
                android:id="@+id/detail_frament3"/>
            <fragment
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:name="com.bwf.p1_landz.ui.onevilla.fragment.Fragment_detail_fragment04"
                android:id="@+id/detail_frament4"/>
            <fragment
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:name="com.bwf.p1_landz.ui.onevilla.fragment.Fragment_detail_fragment05"
                android:id="@+id/detail_frament5"/>
            <fragment
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:name="com.bwf.p1_landz.ui.onevilla.fragment.Fragment_detail_fragment06"
                android:id="@+id/detail_frament6"/>
        </LinearLayout>
    </ScrollView>
    <include
        android:id="@+id/bottom_lianxiren"
        layout="@layout/detail_lianxiren"/>
</RelativeLayout>
